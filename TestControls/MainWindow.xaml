<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:TestControls"
    xmlns:data="clr-namespace:DataAccess;assembly=DataAccess"
    xmlns:ctl="clr-namespace:Infra.Wpf.Controls;assembly=Infra.Wpf"
    xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml" x:Class="TestControls.MainWindow"
    mc:Ignorable="d"
    Title="MainWindow" Height="500" Width="500"
    FontFamily="IRANSans"
    Name="wnd"
    DataContext="{Binding RelativeSource={RelativeSource Self}}">
    
    <Grid>
        <!--<Window.DataContext>
    <local:ViewModel/>
    </Window.DataContext>-->
    <ctl:MultiSelect VerticalAlignment="Center" ItemsSource="{x:Static local:MainWindow.GetDataModel}"/>
    </Grid>
</Window>
